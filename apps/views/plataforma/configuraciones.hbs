              <!-- Correo modificacion -->
              <div class="form-horizontal">
                <div class="col-md-2">
                  <p>
                    <small>Cambia el correo con el que entras a intermed</small>
                  </p>
                </div>
                <div class="form-group">
                  <label for="mailMedic" class="col-md-2 control-label">
                    <span class="glyphicon glyphicon-envelope"></span>
                  </label>
                  <div class="col-md-4">
                    <span class="label label-warning" id="getMail"></span>
                    <div class="input-group">
                      <input type="mail" id="mailMedic" class="form-control" placeholder="E-mail">
                      <span class="input-group-btn">
                        <button class="btn btn-warning" type="button" id="saveMail">
                          <span class="glyphicon glyphicon-floppy-disk"></span>
                        </button>
                      </span>
                    </div>
                  </div>
                </div>
              </div>
              <!-- Fin correo modificacion -->
          </div><!-- fin col-md-12 fin -->
        </div><!-- Fin primer row -->
        <div class="row">
          <div class="col-md-12">
            <!-- URL -->
              <div class="form-horizontal">
                <div class="col-md-2">
                  <p>
                    <small>Cambia el url, esta acción solo se podrá hacer una vez</small>
                  </p>
                </div>
                <div class="form-group">
                  <label for="urlMedic" class="col-md-2 control-label">
                    <span class=""><small>www.intermed.online/</small></span>
                  </label>
                  <div class="col-md-4">
                    <span class="label label-warning"><small>www.intermed.online/</small><small id="getUrl"></small></span>
                  </div>
                  <div class="col-md-4">
                    <div class="input-group">
                      <input type="text" id="urlMedic" title="" class="form-control" placeholder="Url personalizado"/>
                      <span class="input-group-btn">
                        <button class="btn btn-warning" type="button" id="saveUrl">
                          <span class="glyphicon glyphicon-link"></span>
                        </button>
                      </span>
                    </div>
                  </div>
                </div>
              </div>
            <!-- Fin URL -->
          </div><!-- fin col-md-12 -->
