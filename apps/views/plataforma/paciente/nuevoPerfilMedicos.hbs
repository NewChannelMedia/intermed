<header class="header-section">
  <div class="container-fluid header-container">
    <div class="row header-row">
      <div id="profile" class="header-profile col-lg-8 col-md-7 col-sm-10 col-xs-12">
        <div class="header-profile-container">
          <div class="header-profile-photo-container col-lg-4 col-md-5 col-sm-5 col-xs-12">
              <div class="profilePic header-profile-photo-link center-block">
              {{#if usuario.id}}
                <img class="header-profile-photo-img" src="{{usuario.urlFotoPerfil}}"  alt="perfil"/>
              {{else}}
                <img class="header-profile-photo-img" src="{{valSession 'fotoPerfil'}}" id='fotoPerfil'  alt="perfil"/>
                <label for="imageFile">Cambiar Imagen</label>
                <input type="file" id="imageFile" style="display:none">
              {{/if}}
            </div>
          </div>
          <div class="header-profile-body-container col-lg-8 col-md-7 col-sm-7 col-xs-12">
            <div class="header-profile-body-content whiteF flama">
              {{#if usuario.id}}
                <h2 class="profile-name" style="text-transform:capitalize;">
                    {{#if usuario.DatosGenerale}}
                      {{usuario.DatosGenerale.nombre}} {{usuario.DatosGenerale.apellidoP}} {{usuario.DatosGenerale.apellidoM}}
                    {{/if}}
                  </h2>
                <h4 class="profile-location">
                  {{#if usuario.direccion}}
                      {{usuario.direccion.Municipio.municipio}}, {{usuario.direccion.Municipio.Estado.estado}}, MX.
                  {{/if}}</h4>
                <ul class="list-inline profile-esp">
                  {{#each usuario.especialidades}}
                    {{#unless subEsp}}
                      <li>{{Especialidad.especialidad}}</li>
                    {{/unless}}
                  {{/each}}
                </ul>
                <ul class="list-inline profile-subesp">
                  {{#set "subEspPrint"}}{{/set}}
                  {{#each usuario.especialidades}}
                    {{#if subEsp}}
                      {{#ifSessionVal "subEspPrint"}}
                        {{#unset "subEspPrint"}}{{/unset}}
                        <li>
                          <small>Subespecialidad: </small>
                        </li>
                      {{/ifSessionVal}}
                    {{/if}}
                    {{#if subEsp}}
                      <li>
                        <small>{{Especialidad.especialidad}}</small>
                      </li>
                    {{/if}}
                  {{/each}}
                </ul>
              {{else}}
                <h2 class="profile-name" style="text-transform:capitalize;">
                {{#ifSession "name"}}
                  Dr. {{valSession "name"}}
                  {{else}}
                  Aun no tenemos tu nombre registrado, continúa tu registro <a onclick="informacionRegistroMedico()">aquí</a>.
                {{/ifSession}}</h2>
                <h4 class="profile-location">
                  {{#ifSession "municipio"}}
                    {{valSession "municipio"}}, {{valSession "estado"}}. MX
                  {{/ifSession}}</h4>
                <ul class="list-inline profile-esp">
                  {{#eachValSession "especialidades"}}
                    {{#unless subEsp}}
                    <li>
                      {{Especialidad.especialidad}}
                    </li>
                    {{/unless}}
                  {{/eachValSession}}
                </ul>
                <ul class="list-inline profile-subesp">
                  {{#ifsubEsp}}
                    <li>
                      Subespecialidad:
                    </li>
                    {{#eachValSession "especialidades"}}
                      {{#if subEsp}}
                      <li>
                        <small>{{Especialidad.especialidad}}</small>
                      </li>
                      {{/if}}
                    {{/eachValSession}}
                  {{/ifsubEsp}}
                </ul>
              {{/if}}
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</header>

