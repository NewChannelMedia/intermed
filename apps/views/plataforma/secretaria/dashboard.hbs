<form  class="bg-white" method="post" onsubmit="return registrarSecretaria()">
  <div class="container">
    <div class="row">
      <div class="body-container">
        <div class="center-content">
          <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <!--Datos de invitaciÃ³n-->
            <div class="row">
              <div class="col-md-12 text-center">
                <h3 class="h45-light s25 text-uppercase">
                  Dashboard secretaria
                </h3>
                <p>Estos son tus doctores: </p>
              </div>
            </div>


                <div class="row">
                  <div class="col-lg-3 col-md-4">
                    <div class="row">
                      <div class="panel panel-default">
                        <div class="panel-heading">
                            <img class="img-circle" alt="64x64" style="width: 90%;" src="{{#valSession 'fotoPerfil'}}{{/valSession}}" data-holder-rendered="true">
                        </div>
                        <div class="panel-body" style="height: auto;">
                          <h4><strong>{{#valSession 'name'}}{{/valSession}}</strong></h4>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="col-lg-9 col-md-8">

                    <div class="list-group">
                      <div class="list-group-item active s20"><strong>Tus doctores</strong></div>

                      {{#each medicos}}
                      <div class="list-group-item">
                        <p class="s15">
                          <img src="{{Medico.Usuario.urlFotoPerfil}}" style="width:30px" class="img-circle">
                          Dr. {{Medico.Usuario.DatosGenerale.nombre}} {{Medico.Usuario.DatosGenerale.apellidoP}} {{Medico.Usuario.DatosGenerale.apellidoM}}
                        </p>

                      </div>

                      {{/each}}
                    </div>
                  </div>
                </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</form>
